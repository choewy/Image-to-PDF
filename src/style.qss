QPushButton {
    background-color: #ffffff;
    border: 1px solid rgba(200, 200, 200);
    border-radius: 5px;
    height: 30px;
    padding-left: 10px;
    padding-right: 10px;
}

QPushButton:hover {
    border: 2px solid rgba(200, 200, 200);
}

QListWidget {
    background-color: white;
    border: 1px solid rgba(200, 200, 200);
    border-radius: 5px;
}

QTextBrowser {
    background-color: white;
    border: 1px solid rgba(200, 200, 200);
    border-radius: 5px;
}
